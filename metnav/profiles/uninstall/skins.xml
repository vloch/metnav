<?xml version="1.0"?>
<object name="portal_skins">
 <object name="metnav_images" meta_type="Filesystem Directory View"
    directory="metnav:skins/metnav_images" remove="True"/>
 <object name="metnav_portlets" meta_type="Filesystem Directory View"
    directory="metnav:skins/metnav_portlets" remove="True" />
 <object name="metnav_scripts" meta_type="Filesystem Directory View"
    directory="metnav:skins/metnav_scripts" remove="True" />
 <object name="metnav_templates" meta_type="Filesystem Directory View"
    directory="metnav:skins/metnav_templates" remove="True"/>
 <object name="metnav_xquery" meta_type="Filesystem Directory View"
    directory="metnav:skins/metnav_xquery" remove="True"/>
 <object name="metnav_controlpanel" meta_type="Filesystem Directory View"
    directory="metnav:skins/metnav_controlpanel" remove="True"/>
 <skin-path name="*">
  <layer name="metnav_controlpanel" insert-before="custom" remove="True"/>
  <layer name="metnav_images" insert-before="metnav_controlpanel" remove="True"/>
  <layer name="metnav_xquery" insert-before="metnav_images" remove="True"/>
  <layer name="metnav_scripts" insert-before="metnav_xquery" remove="True"/>
  <layer name="metnav_portlets" insert-before="metnav_scripts" remove="True"/>
  <layer name="metnav_templates" insert-before="metnav_portlets" remove="True"/>
  <layer name="custom" insert-before="metnav_templates" remove="True"/>
 </skin-path>
</object>
